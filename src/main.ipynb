    "df = train_df.dropna()\n",
    "\n",
    "# split train and test\n",
    "from sklearn.model_selection import train_test_split\n",
    "\n",
    "X_train, X_test, y_train, y_test = train_test_split(df.drop('fit', axis=1),\n",
    "                                                    df['fit'],\n",
    "                                                    test_size=0.2)\n",
    "\n",
    "# feature engineering\n",
    "from sklearn.compose import ColumnTransformer\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.preprocessing import LabelEncoder, OneHotEncoder, StandardScaler\n",
    "\n",
    "le = LabelEncoder()\n",
    "y_train = le.fit_transform(y_train)\n",
    "y_test = le.transform(y_test)\n",
    "\n",
    "num_cols = X_train.select_dtypes(include=['float64']).columns\n",
    "cat_cols = X_train.select_dtypes(include=['category']).columns\n",
    "\n",
    "num_pipeline = Pipeline([\n",
    "    ('std_scaler', StandardScaler()),\n",
    "])\n",
    "\n",
    "cat_pipeline = Pipeline([\n",
    "    ('one_hot', OneHotEncoder(handle_unknown='ignore')),\n",
    "])\n",
    "\n",
    "full_pipeline = ColumnTransformer([\n",
    "    ('num', num_pipeline, num_cols),\n",
    "    ('cat', cat_pipeline, cat_cols),\n",
    "])\n",
    "\n",
    "X_train_prepared = full_pipeline.fit_transform(X_train)\n",
    "X_test_prepared = full_pipeline.transform(X_test)\n",
    "\n",
    "# over sampling\n",
    "from imblearn.over_sampling import SMOTE\n",
    "\n",
    "smote = SMOTE()\n",
    "X_train_prepared, y_train = smote.fit_resample(X_train_prepared, y_train)\n",
    "\n",
    "# train model\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "\n",
    "log_reg = LogisticRegression(multi_class='multinomial', max_iter=100, C=0.01)\n",
    "log_reg.fit(X_train_prepared, y_train)\n",
    "\n",
    "# evaluate model\n",
    "from sklearn.metrics import accuracy_score, f1_score\n",
    "\n",
    "y_train_pred = log_reg.predict(X_train_prepared)\n",
    "y_test_pred = log_reg.predict(X_test_prepared)\n",
    "\n",
    "pd.concat([\n",
    "    evaluate_model(y_train, y_train_pred, index='train'),\n",
    "    evaluate_model(y_test, y_test_pred, index='test')\n",
    "])"
   ]
